<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>ShoeShop</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <web-module-libraries>
                <library dirs="200">
                    <file>${file.reference.mysql-connector-mysql-connector-java-6.0.6}</file>
                    <path-in-war>WEB-INF/classes</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.java_lib-commons-fileupload-1.4}</file>
                    <path-in-war>WEB-INF/classes</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.java_lib-commons-io-2.6}</file>
                    <path-in-war>WEB-INF/classes</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.java_lib-jstl-1.2}</file>
                    <path-in-war>WEB-INF/classes</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
    </configuration>
</project>
